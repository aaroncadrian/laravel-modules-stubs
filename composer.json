{
    "name": "aaroncadrian/laravel-modules-stubs",
    "description": "Customized stubs to work with laravel-modules",
    "type": "library",
    "require": {
        "illuminate/support": ">=5.5",
        "nwidart/laravel-modules": ">=2.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Aaron Adrian",
            "email": "aaroncadrian@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "AaronAdrian\\LaravelModulesStubs\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AaronAdrian\\LaravelModulesStubs\\LaravelModulesStubsServiceProvider"
            ]
        }
    }
}
